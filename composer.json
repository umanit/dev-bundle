{
  "name": "umanit/dev-bundle",
  "description": "Helper classes and dependencies for usual dev environment",
  "type": "symfony-bundle",
  "keywords": [
    "dev",
    "testing",
    "static analysis"
  ],
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "Umanit\\DevBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Umanit\\DevBundle\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Alexandre Emion",
      "email": "aemion@umanit.fr"
    }
  ],
  "require": {
    "php": "^8.2",
    "symfony/framework-bundle": "^6.4",
    "zenstruck/foundry": "^2.3",
    "spatie/phpunit-watcher": "^1.24",
    "phpunit/phpunit": "^11",
    "phparkitect/phparkitect": "^0.5.3",
    "zenstruck/mailer-test": "^1.4",
    "zenstruck/messenger-test": "^1.11",
    "phpstan/phpstan": "^1.10",
    "doctrine/doctrine-bundle": "^2.8",
    "dama/doctrine-test-bundle": "^8.3",
    "doctrine/orm": "^2.14"
  },
  "require-dev": {
    "phpro/grumphp": "^2.12",
    "squizlabs/php_codesniffer": "^3.12",
    "php-parallel-lint/php-parallel-lint": "^1.4"
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true
    }
  }
}
